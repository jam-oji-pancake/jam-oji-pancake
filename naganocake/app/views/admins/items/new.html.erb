<h2>商品編集</h2>

<%= form_with model:[:admins, @item], local:true do |f| %>

<td>商品画像</td>
<td></td>

<td>商品名</td>
<td><%= f.text_field :name %></td>

<td>商品説明</td>
<td><%= f.text_area :introduction %></td>

<td>ジャンル</td>
<td></td>

<td>税抜き価格</td>
<td><%= f.text_field :price %></td>

<td>販売ステータス</td>
<td></td>

<%= f.submit '新規投稿' %>


<% end %>